<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\DesignLayout" table="design_layout">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="pageName" type="string" column="page_name" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="belongsToSection" type="integer" column="belongs_to_section" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="elementType" type="string" column="element_type" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="orderField" type="integer" column="order_field" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="border" type="string" column="border" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
