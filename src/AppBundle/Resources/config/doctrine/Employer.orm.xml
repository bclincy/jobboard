<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Employer" table="employer">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstName" type="string" column="first_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="last_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="companyName" type="string" column="company_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="companyType" type="integer" column="company_type" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="city" type="integer" column="city" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="zipCode" type="string" column="zip_code" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="integer" column="country" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="state" type="integer" column="state" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="phone" type="string" column="phone" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="website" type="string" column="website" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="profile" type="string" column="profile" length="5000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="active" type="integer" column="active" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="activationCode" type="string" column="activation_code" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="timeCreate" type="datetime" column="time_create" nullable="false"/>
    <field name="timeUpdate" type="datetime" column="time_update" nullable="false"/>
    <field name="lastLogin" type="datetime" column="last_login" nullable="true"/>
    <field name="logo" type="string" column="logo" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="featured" type="smallint" column="featured" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="newRecord" type="boolean" column="new_record" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
