<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\EmployerPlans" table="employer_plans">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="empId" type="integer" column="emp_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="featEmp" type="integer" column="feat_emp" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="featEmpStart" type="datetime" column="feat_emp_start" nullable="true"/>
    <field name="featJobs" type="integer" column="feat_jobs" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="jobs" type="integer" column="jobs" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="membership" type="integer" column="membership" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="membershipStart" type="datetime" column="membership_start" nullable="true"/>
    <field name="resume" type="integer" column="resume" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateCreated" type="datetime" column="date_created" nullable="true"/>
    <field name="dateUpdated" type="datetime" column="date_updated" nullable="true"/>
  </entity>
</doctrine-mapping>
