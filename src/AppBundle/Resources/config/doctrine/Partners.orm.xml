<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Partners" table="partners">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="5000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="website" type="string" column="website" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="logo" type="string" column="logo" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="placement" type="integer" column="placement" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateCreated" type="datetime" column="date_created" nullable="false"/>
    <field name="dateChanged" type="datetime" column="date_changed" nullable="false"/>
  </entity>
</doctrine-mapping>
