<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\PricingPlans" table="pricing_plans">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="timeOrCountId" type="integer" column="time_or_count_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="typePricingId" type="integer" column="type_pricing_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="price" type="decimal" column="price" precision="10" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
