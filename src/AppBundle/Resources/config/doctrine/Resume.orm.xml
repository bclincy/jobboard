<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Resume" table="resume">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="seekerId" type="integer" column="seeker_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="views" type="integer" column="views" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="searchable" type="boolean" column="searchable" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="degree" type="integer" column="degree" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="experience" type="integer" column="experience" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="employeeType" type="integer" column="employee_type" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="positionType" type="integer" column="position_type" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="jobCategory" type="integer" column="job_category" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="minSalary" type="decimal" column="min_salary" precision="10" scale="0" nullable="false"/>
    <field name="toRelocate" type="boolean" column="to_relocate" nullable="false"/>
    <field name="description" type="string" column="description" length="5000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="link" type="string" column="link" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateCreated" type="datetime" column="date_created" nullable="false"/>
    <field name="dateUpdated" type="datetime" column="date_updated" nullable="false"/>
  </entity>
</doctrine-mapping>
