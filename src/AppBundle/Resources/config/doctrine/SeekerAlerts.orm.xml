<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\SeekerAlerts" table="seeker_alerts">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="seekerId" type="integer" column="seeker_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="jobCategory" type="integer" column="job_category" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="country" type="integer" column="country" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="state" type="integer" column="state" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="city" type="integer" column="city" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="keyword" type="string" column="keyword" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateCreated" type="datetime" column="date_created" nullable="true"/>
    <field name="dateUpdated" type="datetime" column="date_updated" nullable="true"/>
  </entity>
</doctrine-mapping>
