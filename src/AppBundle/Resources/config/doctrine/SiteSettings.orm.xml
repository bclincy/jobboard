<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\SiteSettings" table="site_settings">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="companyName" type="string" column="company_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="zipCode" type="string" column="zip_code" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="state" type="string" column="state" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="string" column="country" length="30" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="websiteName" type="string" column="website_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="websiteUrl" type="string" column="website_url" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailServer" type="string" column="email_server" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailUsername" type="string" column="email_username" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailPass" type="string" column="email_pass" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailAddressNoReplay" type="string" column="email_address_no_replay" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailAddressContact" type="string" column="email_address_contact" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailConfiramtion" type="boolean" column="email_confiramtion" nullable="false"/>
    <field name="jobActivation" type="boolean" column="job_activation" nullable="false"/>
    <field name="jobApplication" type="boolean" column="job_application" nullable="false"/>
    <field name="jobDetails" type="boolean" column="job_details" nullable="false"/>
    <field name="freeJobPostings" type="boolean" column="free_job_postings" nullable="true"/>
    <field name="freeResumeSearch" type="boolean" column="free_resume_search" nullable="true"/>
    <field name="paymentProcessor" type="integer" column="payment_processor" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="field1" type="string" column="field1" length="50" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">vendor_id</option>
      </options>
    </field>
    <field name="field2" type="string" column="field2" length="50" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="comment">secret word</option>
      </options>
    </field>
    <field name="appTitle" type="string" column="app_title" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="appKeywords" type="string" column="app_keywords" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="defaultCountry" type="integer" column="default_country" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="appDescription" type="string" column="app_description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="appCurrency" type="string" column="app_currency" length="50" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">USD</option>
      </options>
    </field>
    <field name="gaCode" type="string" column="ga_code" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="siteStaticDesign" type="boolean" column="site_static_design" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
