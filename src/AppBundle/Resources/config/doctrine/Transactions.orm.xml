<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Transactions" table="transactions">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="empId" type="integer" column="emp_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="featEmp" type="integer" column="feat_emp" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="featJobs" type="integer" column="feat_jobs" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="jobs" type="integer" column="jobs" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="membership" type="integer" column="membership" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="resume" type="integer" column="resume" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="totalPrice" type="string" column="total_price" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="smallint" column="status" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="newRecord" type="boolean" column="new_record" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="dateCreated" type="datetime" column="date_created" nullable="true"/>
    <field name="dateUpdated" type="datetime" column="date_updated" nullable="true"/>
  </entity>
</doctrine-mapping>
